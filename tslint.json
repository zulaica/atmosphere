{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [true, 80],
    "no-any": true,
    "no-console": [true],
    "only-arrow-functions": [true],
    "quotemark": ["single"],
    "semicolon": [false],
    "trailing-comma": [false],
    "triple-equals": [true]
  },
  "rulesDirectory": []
}
